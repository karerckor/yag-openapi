{
  "name": "@yag-openapi/core",
  "version": "1.0.0",
  "description": "Core utilities for yag-openapi.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
