    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3",
    "ts-morph": "^26.0.0"
  },
  "peerDependencies": {
    "@elysiajs/eden": "^1.3.2",
    "hono": "^4.9.0"
